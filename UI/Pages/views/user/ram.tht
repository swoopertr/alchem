<div class="container">
    <div class="row">
        <% for (var i = 0 ; i< this.length ;i++) { %>
        <div class="col">
            <div class="card">
                <img src="<% this[i].image %>" class="card-img-top" alt="<% this[i].name %>">
                <div class="card-body">
                    <h5 class="card-title"><% this[i].name %></h5>
                </div>
            </div>
        </div>
        <% } %>
    </div>
</div>
