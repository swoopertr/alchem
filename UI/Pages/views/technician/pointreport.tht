<section class="body">
    <%% header_tech %%>
    
    <div class="inner-wrapper">
        <!-- start: sidebar -->
       <%% sidebar_teknisyen %%>
        <!-- end: sidebar -->

        <section role="main" class="content-body card-margin">
            <header class="page-header">
                <h2>Puan Durumu</h2>               
            </header>

        
            <!-- start: page -->

            <div class="row">
                <div class="col-lg-12">
                    <section class="card">
                        <header class="card-header">
                            <div class="card-actions">
                                <a href="#" class="card-action card-action-toggle" data-card-toggle></a>
                                <a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
                            </div>

                            <h2 class="card-title">Puan GrafiÄŸi</h2>
                            <p class="card-subtitle"></p>
                        </header>
                        <div class="card-body">

                            <!-- Flot: Bars -->
                            <div class="chart chart-md" id="flotBars"></div>
                            
                        </div>
                    </section>
                </div>
              
            </div>

            <div class="row">
                <div class="col">
                    <section class="card">
                        <header class="card-header">
                            <div class="card-actions">
                                <a href="#" class="card-action card-action-toggle" data-card-toggle></a>
                                <a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
                            </div>

                            <h2 class="card-title">Rapor</h2>
                        </header>
                        <div class="card-body">
                            
                            <table class="table table-bordered table-striped mb-0" id="datatable-tabletools">

                                <thead>
                                    <tr>
                                        <th>Girilen Anketler</th>
                                        <th>Zaman</th>
                                        <th>Puan</th>
                                    </tr>
                                </thead>

                                <tbody>
                                    <% for (var i = 0 ; i< this.result.length ;i++) { %>
                                        <tr>
                                            <td><span><% this.result[i].surveyname %></span></td>
                                            <td><span><% this.result[i].CreatedAt.toLocaleDateString() %></span></td>
                                            <td><span><% this.result[i].Score %></span></td>
                                        </tr>      
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </section>
                </div>
            </div>

           

            <!-- end: page -->
        </section>
    </div>

  
</section>
<script>
    var data_  = <% JSON.stringify(this.result) %>;
    debugger;
    var chart_data=[]; 
    for (let i = 0; i < data_.length; i++) {    
        chart_data.push([data_[i].surveyname+'('+ data_[i].CreatedAt.split('T')[0]+')' ,data_[i].Score]);
    }
</script>
